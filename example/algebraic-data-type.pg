-- normal adt
data (Maybe a)
  Just a
  Nohting


-- gadt
data (Option a) where
  {
    Some a : { a -> Option a }
    None : None
  }

-- typeclass
class (Functor a) where
  {
    map : { { a -> b } -> (f a) -> (f b) }
  }

-- instance
instance (FunctorMaybe) (Functor Maybe)
  define (map f m)
    match m
      {
        Nothing => Nothing
        Just a => Just (f a)
      }


